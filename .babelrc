{
  "presets": ["@babel/preset-env"],
  "env": {
    "production": {
      "presets": ["minify"]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["module-resolver", {
      "root": ["./src", "./node_modules"],
      "alias": {
        "test": "./test"
      }
    }]
  ]
}
