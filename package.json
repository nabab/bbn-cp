    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-minify": "^0.5.2",
    "chai": "^4.3.10",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "lessc": "^1.0.2",
