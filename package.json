  ],
  "type": "module",
  "scripts": {
    "transpile:components": "./transpile-components.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:components": "./node_modules/.bin/babel src -o dist/bbn-cp.js && npm run transpile:components && ./node_modules/.bin/webpack --config webpack.config.js",
    "build": "./node_modules/.bin/babel src -o dist/bbn-cp.js && ./node_modules/.bin/webpack --config webpack.config.js",
